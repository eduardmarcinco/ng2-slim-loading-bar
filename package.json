{
  "name": "ng2-slim-loading-bar-observables",
  "description": "Angular2 component shows slim loading bar at the top of the page. Uses Observable.interval() instead of setInterval() to avoid pending macrotask in Angular.",
  "version": "1.0.9",
  "scripts": {
    "test": "karma start",
    "test-watch": "tsc && karma start --no-single-run --auto-watch",
    "commit": "npm run prepublish && npm test && git-cz",
    "prepublish": "ngc && npm run build",
    "build": "webpack && cp style.css bundles/style.css",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eduardmarcinco/ng2-slim-loading-bar.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "slim",
    "loading",
    "bar",
    "progress"
  ],
  "author": "Eduard Marcinco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eduardmarcinco/ng2-slim-loading-bar/issues"
  },
  "main": "bundles/index.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "homepage": "https://github.com/eduardmarcinco/ng2-slim-loading-bar",
  "peerDependencies": {
    "@angular/core": "^7.0.4",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/common": "^7.0.4",
    "@angular/compiler": "^7.0.4",
    "@angular/compiler-cli": "^7.0.4",
    "@angular/core": "^7.0.4",
    "@angular/platform-browser": "^7.0.4",
    "@angular/platform-browser-dynamic": "^7.0.4",
    "@angular/platform-server": "^7.0.4",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "^3.3.0",
    "@types/node": "8.9.4",
    "awesome-typescript-loader": "3.5.0",
    "codelyzer": "^4.5.0",
    "commitizen": "^3.0.4",
    "core-js": "2.5.7",
    "cz-conventional-changelog": "^2.1.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "loader-utils": "^1.1.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "semantic-release": "^15.12.0",
    "source-map-loader": "^0.2.4",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.1.6",
    "webpack": "^3.12.0",
    "zone.js": "^0.8.26"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/cli": "^7.0.6"
  }
}

{
  "name": "ng2-slim-loading-bar-observables",
  "description": "Angular2 component shows slim loading bar at the top of the page. Uses Observable.interval() instead of setInterval() to avoid pending macrotask in Angular.",
  "version": "1.0.8",
  "scripts": {
    "test": "karma start",
    "test-watch": "tsc && karma start --no-single-run --auto-watch",
    "commit": "npm run prepublish && npm test && git-cz",
    "prepublish": "ngc && npm run build",
    "build": "webpack && cp style.css bundles/style.css",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eduardmarcinco/ng2-slim-loading-bar.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "slim",
    "loading",
    "bar",
    "progress"
  ],
  "author": "Eduard Marcinco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eduardmarcinco/ng2-slim-loading-bar/issues"
  },
  "main": "bundles/index.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "homepage": "https://github.com/eduardmarcinco/ng2-slim-loading-bar",
  "peerDependencies": {
    "@angular/core": "^6.1.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@angular/animations": "6.1.0",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/compiler-cli": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/platform-server": "6.1.0",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "2.8.4",
    "@types/node": "8.9.4",
    "awesome-typescript-loader": "3.5.0",
    "codelyzer": "^4.4.2",
    "commitizen": "2.8.6",
    "core-js": "2.4.1",
    "cz-conventional-changelog": "1.2.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "^2.1.0",
    "karma-remap-coverage": "~0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "loader-utils": "~0.2.16",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^6.0.0",
    "semantic-release": "^8.2.0",
    "source-map-loader": "0.1.5",
    "ts-helpers": "1.1.2",
    "tslint": "~5.9.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.9.2",
    "webpack": "3.12.0",
    "zone.js": "^0.8.26"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/cli": "^6.0.8"
  }
}
